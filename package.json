    "prebuild": "node extractLicences.js",
    "build": "vite build",
    "postinstall": "license-checker --production --json > ./src/assets/licences.json"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "license-checker": "^25.0.1",
    "vite": "^6.3.6"
  }
