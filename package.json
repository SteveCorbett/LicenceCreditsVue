{
  "name": "licence-credits-vue",
  "private": false,
  "publisher": "Corbett Technologies Pty Ltd",
  "author": "Stephen Corbett",
  "version": "1.0.0",
  "description": "A Vue 3 app to display the licences of all the dependencies in a project.",
  "license": "0BSD",
  "keywords": [
    "licence",
    "license",
    "vue",
    "vite"
  ],
  "type": "module",
  "scripts": {
